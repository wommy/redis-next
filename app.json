{
  "name": "Redis Shopping Cart",
  "description": "This shopping cart is using Redis and RedisJson module functionalities, allowing to save JSON as keys using methods like json_get and json_set.",
  "repository": "https://github.com/redis-developer/basic-redis-shopping-chart-nodejs",
  "keywords": [
    "NodeJS",
    "ExpressJS",
    "Redis",
    "Shopping Cart",
    "json_get",
    "json_set"
  ],
  "env": {
    "REDIS_ENDPOINT_URI": {
      "description": "Redis server URI",
      "required": true
    },
    "REDIS_PASSWORD": {
      "description": "Redis password",
      "required": true
    }
  },
  "stack": "container"
}
